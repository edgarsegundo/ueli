{
    "name": "vue-3-electron-vite-2",
    "private": true,
    "version": "0.0.0",
    "main": "dist/main/index.cjs",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/oliverschwendener/ueli"
    },
    "homepage": "https://ueli.app",
    "author": {
        "name": "Oliver Schwendener",
        "email": "oliver.schwendener@proton.me",
        "url": "https://github.com/oliverschwendener"
    },
    "description": "This is a keystroke launcher for Windows and macOS.",
    "os": [
        "win32",
        "darwin"
    ],
    "keywords": [
        "keystroke",
        "launcher",
        "search",
        "Windows",
        "macOS"
    ],
    "scripts": {
        "dev:renderer": "vite src/renderer",
        "build:renderer": "vue-tsc -p src/renderer/tsconfig.json --noEmit && vite build src/renderer",
        "build:renderer-app": "vue-tsc -p src/renderer-app/tsconfig.json --noEmit && vite build src/renderer-app",
        "build:preload": "tsc -p src/preload/tsconfig.json --noEmit && vite build src/preload",
        "build:main": "tsc -p src/main/tsconfig.json --noEmit && vite build src/main",
        "build": "npm run build:renderer && npm run build:renderer-app && npm run build:preload && npm run build:main",
        "dev": "cross-env NODE_ENV=development ts-node scripts/dev-server.ts",
        "start": "electron . --remote-debugging-port=9223"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "6.2.1",
        "@vueuse/electron": "^10.6.0",
        "bulma": "0.9.4",
        "bulma-extensions": "6.2.7",
        "lorem-ipsum": "2.0.8",
        "mitt": "^3.0.1",
        "pinia": "^2.1.7",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@types/node-powershell": "3.1.0",
        "@types/sqlite3": "3.1.8",
        "@types/vue-color": "2.4.4",
        "@vitejs/plugin-vue": "^4.2.3",
        "axios": "1.2.4",
        "color": "4.2.3",
        "cross-env": "^7.0.3",
        "electron": "v26.5.0",
        "electron-store": "^8.1.0",
        "electron-updater": "6.1.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vue-tsc": "^1.8.5"
    }
}
